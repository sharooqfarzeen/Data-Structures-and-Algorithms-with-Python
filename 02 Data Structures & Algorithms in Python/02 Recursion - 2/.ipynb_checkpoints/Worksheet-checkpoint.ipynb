   "source": [
    "### Replace all 'a's with 'b's"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "688c21de",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "'bbcaa'"
      ]
     },
     "execution_count": 10,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "def replacer(string, curr = 0):\n",
    "    if curr == len(string):\n",
    "        return\n",
    "    replacer(string, curr+1)\n",
    "    if string[curr] == 'a':\n",
    "        string = string[:curr] + 'b' + string[curr+1:]\n",
    "    return string\n",
    "\n",
    "replacer('abcaa')\n",
    "        "
   ]
  }
